# Frontend environment example for Nutrition Connect

# Enable Supabase Auth for the frontend
REACT_APP_USE_SUPABASE_AUTH=true

# Supabase project URL and anon key
REACT_APP_SUPABASE_URL=https://nznsixbvloaorucujewu.supabase.co
REACT_APP_SUPABASE_ANON_KEY=<TO_BE_FILLED_BY_USER_OR_DEPLOYER>

# Optional: API/WS base URLs to backend gateway (leave empty to use relative)
REACT_APP_API_BASE_URL=
REACT_APP_WS_BASE_URL=

# Storage bucket name (storage remains disabled on backend as per request)
REACT_APP_STORAGE_BUCKET=uploads

# Site URL for auth email redirects (falls back to window.location.origin if not set)
REACT_APP_SITE_URL=
